# Link Time Optimization
EXTRAFLAGS += -flto

# Bootloader selection
BOOTLOADER = halfkay

# Auto Shift - hold key to shift
AUTO_SHIFT_ENABLE = yes

# Combos - key chords
COMBO_ENABLE=yes

# Caps Words - a smarter caps lock
# ... that doesn't play super will with tapdance
# when tapdance is on alpha keys or minus
CAPS_WORD_ENABLE = yes

